{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
      "runtimeArgs": [
        "--transpile-only",
        "--respawn",
        "--inspect=9229",
        "src/main.ts"
      ],
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart",
      "port": 9229
    }
  ]
}
